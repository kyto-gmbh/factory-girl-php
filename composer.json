{
    "name": "breerly/factory-girl-php",
    "type": "library",
    "description": "Fixture replacement for focused and readable tests - A PHP port of Thoughtbot's Ruby Factory Girl",
    "keywords": [
        "factory girl",
        "fixture factory",
        "fixture replacement",
        "object mother",
        "testing",
        "fixture",
        "tdd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Grayson Koonce",
            "email": "breerly@gmail.com"
        },
        {
            "name": "Kyto GmbH",
            "email": "it@kyto.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "doctrine/orm": ">=2.5.6,<2.6-dev",
        "doctrine/dbal": ">=2.5.7,<2.6-dev",
        "doctrine/common": ">=2.7.2,<2.8-dev"
    },
    "autoload": {
        "psr-0": {
            "FactoryGirl": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "FactoryGirl\\Tests": "tests",
            "Doctrine\\Tests": "vendor/doctrine/dbal/tests"
        }
    }
}
